        {
          "type": "exit",
          "style": "diamond",
          "powerline_symbol": "",
          "invert_powerline": false,
          "foreground": "#ffffff",
          "foreground_templates": null,
          "background": "#2e9599",
          "background_templates": null,
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "always_enabled": true,
            "color_background": true,
            "display_exit_code": false,
            "error_color": "#f1184c",
            "prefix": "\u003ctransparent\u003e\u003c/\u003e \ue23a"
