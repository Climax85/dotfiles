          "foreground": "#ffffff",
          "foreground_templates": null,
          "background": "#ff479c",
          "background_templates": null,
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
						"enable_hyperlink": true,
            "prefix": "  ",
						"style": "folder",
						"mapped_locations": [
						]
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "",
          "invert_powerline": false,
          "foreground": "#193549",
          "foreground_templates": null,
          "background": "#fffb38",
          "background_templates": null,
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "display_stash_count": true,
            "display_upstream_icon": true
          }
        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "",
          "invert_powerline": false,
          "foreground": "#ffffff",
          "foreground_templates": null,
          "background": "#6CA35E",
          "background_templates": null,
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "prefix": " \uE718 "
          }
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#000000",
          "background": "#00ffff",
          "properties": {
            "prefix": " \uE77F "
          }
        },
				{
					"type": "az",
					"style": "powerline",
					"powerline_symbol": "\uE0B0",
					"foreground": "#000000",
					"background": "#9ec3f0",
					"properties": {
						"display_id": true,
						"display_name": true,
						"info_separator": " @ ",
						"prefix": " \uFD03 "
					}
				},
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "",
          "invert_powerline": false,
